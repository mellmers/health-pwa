{
  "name": "health-pwa",
  "version": "1.2.0",
  "description": "Health PWA using Material UI with PawJS",
  "scripts": {
    "start": "pawjs --env=development --env-config-path=./.env start",
    "startApi": "nodemon --exec babel-node src/api/index.js",
    "startProduction": "node ./dist/server.js",
    "build": "pawjs --env=production --config=prod.pawconfig.json build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mellmers/health-pwa.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/mellmers/health-pwa#readme",
  "author": {
    "name": "Moritz Ellmers",
    "email": "info@moritzellmers.de",
    "url": "https://moritzellmers.de"
  },
  "engines": {
    "node": ">=10.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@pawjs/pawjs": "^2.3.2",
    "@pawjs/redux": "^2.2.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.4",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "nodemon": "^1.19.1",
    "react-jss": "^8.6.1",
    "recharts": "^1.3.4"
  }
}
